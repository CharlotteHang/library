<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="books" prefHeight="400.0" prefWidth="640.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.ubc.cs.cpsc210.controller.BookPageController">
  <stylesheets>
    <URL value="@../../../../../BasicApplication_css/BasicApplication.css" />
  </stylesheets>
   <children>
      <JFXButton fx:id="back" onAction="#backPage" ripplerFill="#08bbf2" text="Back">
         <VBox.margin>
            <Insets left="30.0" top="20.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="returnbook" onAction="#returnBook" text="Return">
         <VBox.margin>
            <Insets left="50.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="borrowbook" onAction="#borrowBook" text="Borrow">
         <VBox.margin>
            <Insets left="200.0" top="-25.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="subscribebook" onAction="#subscribeBook" text="Subscribe">
         <VBox.margin>
            <Insets left="350.0" top="-25.0" />
         </VBox.margin>
      </JFXButton>
      <JFXButton fx:id="unscribebook" onAction="#unscribeBook" text="Unsubcribe">
         <VBox.margin>
            <Insets left="500.0" top="-25.0" />
         </VBox.margin>
      </JFXButton>
      <JFXTextArea prefHeight="309.0" prefWidth="640.0" />
      <ScrollPane fx:id="pane" prefHeight="1500.0" prefWidth="200.0">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="2000.0" prefWidth="200.0" />
         </content>
      </ScrollPane>
   </children>
</fx:root>
